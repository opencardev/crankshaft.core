{
  "id": "com.opencardev.sample",
  "name": "Sample Extension",
  "version": "1.0.0",
  "entrypoint": "./run-sample-extension.sh",
  "permissions": [
    "ui.tile",
    "media.source",
    "network"
  ],
  "default_locale": "en-GB",
  "ui_tiles": [
    {
      "id": "sample-tile",
      "title": "Sample Extension",
      "icon": "sample-icon.svg"
    }
  ],
  "background_services": [
    "com.opencardev.sample.MediaService"
  ]
}
